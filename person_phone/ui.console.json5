{
  routes: {
    'person/:id/*': {
      components: {
        sidebar: {
          items2: [
            {
              component: 'Link',
              properties: {
                label: 'Phones',
                to: 'phones'
              }
            }
          ]
        },
        main: [
          {
            path: 'phones',
            label: 'Phones',
            component: 'RecordDisplay',
            properties: {
              table: 'person_phone',
              conditions: [{ eql: 'person_id={{person_id}}' }],
              rows: ['{{phone}}', '{{phone_type}}']
            }
          }
        ]
      }
    }
  }
}
